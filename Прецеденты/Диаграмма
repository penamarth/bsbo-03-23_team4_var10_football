@startuml
actor Администратор

activate Register

Администратор -> Register : makeReport()

Register -> Store : getHistory()
Register <- Store : list_history

Register -> Report : create()
activate Report


Register -> Report : makeReport(list_history)
Register <- Report : report


Администратор <- Register : report

Администратор -> Register : closeReport()
Register -> Report : closeReport()
Report -> Register : true
deactivate Report

@enduml
